      "material": {
        "solidColor": {
          "color": {
            "rgba": [0, 255, 0, 50]
          }
        }
      },
      "height": 0,
      "outline": true,
      "outlineColor": {
        "rgba": [0, 0, 0, 255]
      }
    }
  }
