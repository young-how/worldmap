[
  {
    "id": "document",
    "name": "CZML Colors",
    "version": "1.0"
  },
  {
    "id": "rgba",
    "name": "Rectangle with outline using RGBA Colors",
    "rectangle": {
      "coordinates": {
        "wsenDegrees": [@longitude1@, @latitude1@, @longitude2@, @latitude2@]
